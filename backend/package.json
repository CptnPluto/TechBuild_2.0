{
  "name": "techbuild-backend",
  "version": "2.0.0",
  "description": "TechBuild 2.0 Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.4.7",
    "axios": "^1.7.9",
    "winston": "^3.17.0",
    "redis": "^4.7.0",
    "ioredis": "^5.4.2",
    "minio": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "zod": "^3.24.1",
    "uuid": "^11.0.7",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.17.14",
    "@types/express": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.12",
    "@types/multer": "^1.4.13",
    "@types/uuid": "^10.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "prisma": "^6.12.0"
  },
  "keywords": [
    "construction",
    "takeoff",
    "ai",
    "document-processing",
    "express",
    "typescript"
  ],
  "author": "TechBuild 2.0",
  "license": "ISC"
}